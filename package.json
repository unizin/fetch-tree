{
  "name": "fetch-tree",
  "version": "1.1.2",
  "description": "",
  "main": "lib/index.js",
  "scripts": {
    "build": "BABEL_ENV=production babel src -d lib --ignore __tests__",
    "lint": "eslint --ext .js --ext .jsx src/",
    "prepublish": "npm run build",
    "report": "nyc report --reporter=html",
    "test": "nyc ava"
  },
  "ava": {
    "files": [
      "src/**/__tests__/*.js*"
    ],
    "source": [
      "src/**/*.{js,jsx}",
      "!dist/**/*"
    ],
    "failFast": true,
    "tap": true,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unizin/fetch-tree.git"
  },
  "author": "Unizin, Ltd.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/unizin/fetch-tree/issues"
  },
  "homepage": "https://github.com/unizin/fetch-tree#readme",
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.9.2",
    "eslint-plugin-jsx-a11y": "^1.5.3",
    "eslint-plugin-react": "^5.2.2",
    "nyc": "^6.6.1",
    "react": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "testdouble": "^1.4.3"
  },
  "peerDependencies": {
    "react": "^15 || ^0.14",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2"
  }
}
